<table style='width:100%; text-align:left; border: 1px solid #ccc; margin:0; padding:0; border-collapse: collapse;'>
  <tr>
    <th style='border: 1px solid #ccc;'>Date</th>
    <th style='border: 1px solid #ccc;'>Unique Users</th>
  </tr>
  {{#each uniqueUsersByDate}}
  <tr>
    <td style='border: 1px solid #ccc;'>{{{this.date}}}</td>
    <td style='border: 1px solid #ccc;'>{{{this.uniqueUsers}}}</td>
  </tr>
  {{/each}}
</table>